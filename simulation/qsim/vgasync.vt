module vgasync_vlg_vec_tst();
// constants                                           
// general purpose registers
reg CLOCK_50;
// wires                                               
wire [3:0] VGA_B;
wire [3:0] VGA_G;
wire [0:0] VGA_HS;
wire [3:0] VGA_R;
wire [0:0] VGA_VS;

wire sampler;                             

// assign statements (if any)                          
vgasync i1 (
// port map - connection between master ports and signals/registers   
	.CLOCK_50(CLOCK_50),
	.VGA_B(VGA_B),
	.VGA_G(VGA_G),
	.VGA_HS(VGA_HS),
	.VGA_R(VGA_R),
	.VGA_VS(VGA_VS)
);

// CLOCK_50
always
begin
	CLOCK_50 = 1'b0;
	CLOCK_50 = #5000 1'b1;
	#5000;
end 

vgasync_vlg_sample_tst tb_sample (
	.CLOCK_50(CLOCK_50),
	.sampler_tx(sampler)
);

vgasync_vlg_check_tst tb_out(
	.VGA_B(VGA_B),
	.VGA_G(VGA_G),
	.VGA_HS(VGA_HS),
	.VGA_R(VGA_R),
	.VGA_VS(VGA_VS),
	.sampler_rx(sampler)
);
endmodule

